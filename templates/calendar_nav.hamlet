$if null cals
   <i>(empty)
$else
 <div .cal-view.nav-stats>
  <ul>
    $forall (Entity k c, act, n) <- cals
      <li>
         $if act
          <a .cal-action .icon-white.icon-eye-open href=@{CalendarViewR k "unview"}>
         $else
          <a .cal-action .icon-white.icon-eye-close href=@{CalendarViewR k "view"}>

         $if (==) mcal (Just k)
            <b>#{calendarName c}
         $else
            <a .color-blue href=@{CalendarActiveR k}>
               <b>#{calendarName c}
         <sup .label .label-#{calendarColor c}>#{show n}
